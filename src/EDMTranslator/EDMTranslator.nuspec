<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>EDMTranslator</id>
    <version>$PackageVersion$</version>
    <title>EDMTranslator</title>
    <authors>sappho192</authors>
    <owners>sappho192</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <!-- <icon>icon.png</icon> -->
    <projectUrl>https://github.com/sappho192/EDMTranslator</projectUrl>
    <repository type="git" url="https://github.com/sappho192/EDMTranslator.git" branch="$RepositoryBranch$" commit="$RepositoryCommit$" />
    <description>Text translator library based on LLM models, especially EncoderDecoderModel in HuggingFace</description>
    <copyright>Copyright (c) Taein Kim(sappho192)</copyright>
    <tags>library;translator;llm;bert;gpt;japanese;korean</tags>
    <readme>docs\README.md</readme>
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="BertJapaneseTokenizer" version="1.0.8" />
        <dependency id="Microsoft.ML.OnnxRuntime" version="1.17.3" />
        <dependency id="NumSharp" version="0.30.0" />
        <dependency id="Tokenizers.DotNet" version="0.9.2" />
      </group>
      <group targetFramework="net7.0">
        <dependency id="BertJapaneseTokenizer" version="1.0.8" />
        <dependency id="Microsoft.ML.OnnxRuntime" version="1.17.3" />
        <dependency id="NumSharp" version="0.30.0" />
        <dependency id="Tokenizers.DotNet" version="0.9.2" />
      </group>
      <group targetFramework="net8.0">
        <dependency id="BertJapaneseTokenizer" version="1.0.8" />
        <dependency id="Microsoft.ML.OnnxRuntime" version="1.17.3" />
        <dependency id="NumSharp" version="0.30.0" />
        <dependency id="Tokenizers.DotNet" version="0.9.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\README.md" target="docs\" />
    <file src="bin\$Configuration$\net6.0\EDMTranslator.*" target="lib/net6.0" />
    <file src="bin\$Configuration$\net7.0\EDMTranslator.*" target="lib/net7.0" />
    <file src="bin\$Configuration$\net8.0\EDMTranslator.*" target="lib/net8.0" />
  </files>
</package>